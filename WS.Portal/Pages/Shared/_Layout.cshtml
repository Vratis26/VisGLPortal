<!DOCTYPE html>
<html lang="cs">
<head>
    <meta charset="utf-8">

    <title>Vratis WS</title>

    <meta content="ie=edge" http-equiv="x-ua-compatible">

    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css">
    <meta name="msapplication-TileColor" content="#ffffff">
    <meta name="msapplication-TileImage" content="static/images/favicon-160x160.png">

    <link rel="stylesheet" href="~/css/pure-min.css">
    <link rel="stylesheet" href="~/css/grids-responsive-min.css">
    <link rel="stylesheet" href="~/css/benteler.css">
    <link rel="stylesheet" href="~/lib/font-awesome/css/all.css">

    <link rel="stylesheet" href="~/css/main.css">
    <link rel="stylesheet" href="~/css/site.css" />

    

</head>
<body>
    <div class="container">
        <div id="main-content-wrapper">

            <header id="header">
                <div id="header-wrapper">
                    <nav class="navbar navbar-default">
                        <div class="w-100 px-3">
                            <div class="float-left">
                                <a href="/Index">
                                    Vratis app
                                </a>
                            </div>
                            <div class="float-right">
                            </div>
                        </div>



                        <div class="navbar-collapse" id="benteler-main-nav-dropdown">
                            <ul class="main-nav nav navbar-nav d-inline float-left">
                                <li class="mr-3 internal-link px-3"><a asp-page="/Visualization/Index">Visualiztion</a></li>
                                <li class="mr-3 internal-link px-3"><a asp-page="/Index">Editation</a></li>
                            </ul>
                        </div>
                    </nav>
                </div>
            </header>
        </div>
    </div>

    @RenderBody()

    <div class="container">
        <div id="main-content-wrapper">
            <footer class="row">
                <div id="footer" class="pure-u-1 container-fluid gray">
                    <div id="footer-wrapper">
                        <div id="footer-data">
                            Vratis app
                        </div>
                    </div>
                    <div id="footer-bg"></div>
                </div>
            </footer>
        </div>
    </div>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/js/popper.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.min.js"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>